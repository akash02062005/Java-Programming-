import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int []arr=new int[n];
    int flag=0;
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    int target=sc.nextInt();
    for(int i=0;i<n-1;i++){
        if(target==arr[i]){
            flag=1;
            break;
        }
        }

    if(flag!=0)
        System.out.printf("%d is presented in an array.",target);
    else
        System.out.printf("%d is not presented in an array.",target);
    }
                      
}
